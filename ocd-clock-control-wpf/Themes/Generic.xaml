<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:PresentationOptions="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"  
    xmlns:local="clr-namespace:OCDClock">

    <local:MilliSecondsConverter x:Key="milliConverter"/>    
    <local:SecondsConverter x:Key="secondsConverter"/>
    <local:MinutesConverter x:Key="minutesConverter"/>
    <local:HoursConverter x:Key="hoursConverter"/>

    <Style TargetType="{x:Type local:ClockControl}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type local:ClockControl}">
                    <Canvas x:Name="MyContainer" Width="100" Height="100">
                        <Image Stretch="None" HorizontalAlignment="Left">
                            <Image.Source>
                                <DrawingImage PresentationOptions:Freeze="True">
                                    <DrawingImage.Drawing>
                                        <DrawingGroup x:Name="geoContainer">

                                            <!-- The outer clock circle. -->

                                            <GeometryDrawing>
                                                <GeometryDrawing.Geometry>
                                                    <GeometryGroup>
                                                        <EllipseGeometry Center="50,50" RadiusX="47.5" RadiusY="47.5" />
                                                    </GeometryGroup>
                                                </GeometryDrawing.Geometry>
                                                <GeometryDrawing.Pen>
                                                    <Pen Thickness="5" Brush="Black" />
                                                </GeometryDrawing.Pen>
                                            </GeometryDrawing>

                                            <!-- Minor tick marks (minutes) around the edge of the clock. -->
                                            
                                            <GeometryDrawing>
                                                <GeometryDrawing.Geometry>
                                                    <GeometryGroup>
                                                    
                                                        <LineGeometry StartPoint="50, 10" EndPoint="50,5"></LineGeometry>
                                                        <LineGeometry StartPoint="50, 10" EndPoint="50,5">
                                                            <LineGeometry.Transform>
                                                                <RotateTransform CenterX="50" CenterY="50" Angle="6" />
                                                            </LineGeometry.Transform>
                                                        </LineGeometry>
                                                        <LineGeometry StartPoint="50, 10" EndPoint="50,5">
                                                            <LineGeometry.Transform>
                                                                <RotateTransform CenterX="50" CenterY="50" Angle="12" />
                                                            </LineGeometry.Transform>
                                                        </LineGeometry>
                                                        <LineGeometry StartPoint="50, 10" EndPoint="50,5">
                                                            <LineGeometry.Transform>
                                                                <RotateTransform CenterX="50" CenterY="50" Angle="18" />
                                                            </LineGeometry.Transform>
                                                        </LineGeometry>
                                                        <LineGeometry StartPoint="50, 10" EndPoint="50,5">
                                                            <LineGeometry.Transform>
                                                                <RotateTransform CenterX="50" CenterY="50" Angle="24" />
                                                            </LineGeometry.Transform>
                                                        </LineGeometry>

                                                        <LineGeometry StartPoint="50, 10" EndPoint="50,5">
                                                            <LineGeometry.Transform>
                                                                <RotateTransform CenterX="50" CenterY="50" Angle="30" />
                                                            </LineGeometry.Transform>
                                                        </LineGeometry>
                                                        <LineGeometry StartPoint="50, 10" EndPoint="50,5">
                                                            <LineGeometry.Transform>
                                                                <RotateTransform CenterX="50" CenterY="50" Angle="36" />
                                                            </LineGeometry.Transform>
                                                        </LineGeometry>
                                                        <LineGeometry StartPoint="50, 10" EndPoint="50,5">
                                                            <LineGeometry.Transform>
                                                                <RotateTransform CenterX="50" CenterY="50" Angle="42" />
                                                            </LineGeometry.Transform>
                                                        </LineGeometry>
                                                        <LineGeometry StartPoint="50, 10" EndPoint="50,5">
                                                            <LineGeometry.Transform>
                                                                <RotateTransform CenterX="50" CenterY="50" Angle="48" />
                                                            </LineGeometry.Transform>
                                                        </LineGeometry>
                                                        <LineGeometry StartPoint="50, 10" EndPoint="50,5">
                                                            <LineGeometry.Transform>
                                                                <RotateTransform CenterX="50" CenterY="50" Angle="54" />
                                                            </LineGeometry.Transform>
                                                        </LineGeometry>

                                                        <LineGeometry StartPoint="50, 10" EndPoint="50,5">
                                                            <LineGeometry.Transform>
                                                                <RotateTransform CenterX="50" CenterY="50" Angle="60" />
                                                            </LineGeometry.Transform>
                                                        </LineGeometry>
                                                        <LineGeometry StartPoint="50, 10" EndPoint="50,5">
                                                            <LineGeometry.Transform>
                                                                <RotateTransform CenterX="50" CenterY="50" Angle="66" />
                                                            </LineGeometry.Transform>
                                                        </LineGeometry>
                                                        <LineGeometry StartPoint="50, 10" EndPoint="50,5">
                                                            <LineGeometry.Transform>
                                                                <RotateTransform CenterX="50" CenterY="50" Angle="72" />
                                                            </LineGeometry.Transform>
                                                        </LineGeometry>
                                                        <LineGeometry StartPoint="50, 10" EndPoint="50,5">
                                                            <LineGeometry.Transform>
                                                                <RotateTransform CenterX="50" CenterY="50" Angle="78" />
                                                            </LineGeometry.Transform>
                                                        </LineGeometry>
                                                        <LineGeometry StartPoint="50, 10" EndPoint="50,5">
                                                            <LineGeometry.Transform>
                                                                <RotateTransform CenterX="50" CenterY="50" Angle="84" />
                                                            </LineGeometry.Transform>
                                                        </LineGeometry>

                                                        <LineGeometry StartPoint="50, 10" EndPoint="50,5">
                                                            <LineGeometry.Transform>
                                                                <RotateTransform CenterX="50" CenterY="50" Angle="90" />
                                                            </LineGeometry.Transform>
                                                        </LineGeometry>
                                                        <LineGeometry StartPoint="50, 10" EndPoint="50,5">
                                                            <LineGeometry.Transform>
                                                                <RotateTransform CenterX="50" CenterY="50" Angle="96" />
                                                            </LineGeometry.Transform>
                                                        </LineGeometry>
