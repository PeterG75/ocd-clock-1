                                                <GeometryDrawing.Geometry>
                                                    <GeometryGroup>
                                                        <LineGeometry StartPoint="50,53" EndPoint="50, 25"></LineGeometry>
                                                        <GeometryGroup.Transform>
                                                            <RotateTransform Angle="{Binding Path=DateTime, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource hoursConverter}}" CenterX="50" CenterY="50">
                                                            </RotateTransform>
                                                        </GeometryGroup.Transform>
                                                    </GeometryGroup>
                                                </GeometryDrawing.Geometry>
                                                <GeometryDrawing.Pen>
                                                    <Pen Thickness="5" Brush="Black" EndLineCap="Triangle" />
                                                </GeometryDrawing.Pen>
                                            </GeometryDrawing>
                                            
                                            <!-- The minute hand -->

                                            <GeometryDrawing>
                                                <GeometryDrawing.Geometry>
                                                    <GeometryGroup>
                                                        <LineGeometry StartPoint="50,53" EndPoint="50, 14"></LineGeometry>
                                                        <GeometryGroup.Transform>
                                                            <RotateTransform Angle="{Binding Path=DateTime, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource minutesConverter}}" CenterX="50" CenterY="50">
                                                            </RotateTransform>
                                                        </GeometryGroup.Transform>
                                                    </GeometryGroup>
                                                </GeometryDrawing.Geometry>
                                                <GeometryDrawing.Pen>
